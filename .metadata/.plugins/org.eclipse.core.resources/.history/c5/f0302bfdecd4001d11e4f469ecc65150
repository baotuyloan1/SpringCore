package edu.fa;

import java.util.List;

import edu.fa.model.Student;
import edu.fa.service.StudentService;
import edu.fa.service.StudentServiceImpl;

public class EducationApp {

	public static void main(String[] args) {
		StudentService studentServiceImpl = new StudentServiceImpl();
		List<Student> studentList = studentServiceImpl.getALlStudent();
		System.out.println(studentList.size());
		Student student = studentList.get(0);
		System.out.println(student);
	}

}
